<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Popover
        id="popover"
        placement="Bottom"
    >
        <Table
            id="_IDGenTable1"
            width="700px"
        >
            <columns>
                <Column
                    id="_IDGenColumn1"
                    width="7rem"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText1"
                        text="Material"
                    />
                </Column>
                <Column
                    id="_IDGenColumn2"
                    width="7rem"
                    demandPopin="true"
                >
                    <Text
                        id="_IDGenText2"
                        text="Quantity"
                    />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1">
                    <cells>
                        <ObjectIdentifier
                            id="_IDGenObjectIdentifier1"
                            title="{AsnNumModel>Matnr}"
                            text="{AsnNumModel>Maktx}"
                        />
                        <Text
                            id="_IDGenText4"
                            class="asnNum"
                            text="{AsnNumModel>Asn}/{AsnNumModel>AsnYear}"
                        />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Popover>
</core:FragmentDefinition>
